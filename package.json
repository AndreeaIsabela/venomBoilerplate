    "express-jwt": "^5.3.1",
    "font-awesome": "^4.7.0",
    "font-roboto": "0.0.2",
    "jsonwebtoken": "^8.3.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2",
    "mongoose": "^5.1.7",
    "multer": "^1.3.1",
    "nodemailer": "^6.2.1",
    "typescript": "^3.0.1",
